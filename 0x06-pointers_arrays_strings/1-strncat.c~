#include "main.h"
/**
 *_strncat - concatinates source with destination by the value of n
 *@dest: is destination
 *@src: is source
 *@n: is integer
 *Return: returns the concatinated value
 */
char *_strncat(char *dest, char *src, int n)
{
	int i = 0;
	int j;

	while (dest[i] != '\0')
		i++;

	for (j = 0; j != n; j++)
	{
		dest[i] = src[j];
		i++;
	}
	dest[i] = src[j];
	return (dest);
}
